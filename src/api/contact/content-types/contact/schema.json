{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "Contact",
    "description": "Contact form submissions"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "subject": {
      "type": "enumeration",
      "enum": [
        "genel",
        "bagis",
        "gonullu",
        "proje",
        "diger"
      ],
      "required": true
    },
    "message": {
      "type": "text",
      "required": true
    },
    "contactStatus": {
      "type": "enumeration",
      "enum": [
        "new",
        "read",
        "replied"
      ],
      "default": "new",
      "required": true
    }
  }
}
